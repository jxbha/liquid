[workers]
%{ for k,v in workers ~}
${k} ansible_host=${v}
%{ endfor ~}

[workers:vars]
ansible_user=${user}
ansible_ssh_private_key_file=${ssh_key}

[controllers]
%{ for k,v in controllers ~}
${k} ansible_host=${v}
%{ endfor ~}

[controllers:vars]
ansible_user=${user}
ansible_ssh_private_key_file=${ssh_key}
